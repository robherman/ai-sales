{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "paths": {
      "@lib/chat": [
        "libs/chat/src"
      ],
      "@lib/chat/*": [
        "libs/chat/src/*"
      ],
      "@lib/config": [
        "libs/config/src"
      ],
      "@lib/config/*": [
        "libs/config/src/*"
      ],
      "@lib/customers": [
        "libs/customers/src"
      ],
      "@lib/customers/*": [
        "libs/customers/src/*"
      ],
      "@lib/integrations": [
        "libs/integrations/src"
      ],
      "@lib/integrations/*": [
        "libs/integrations/src/*"
      ],
      "@lib/orders": [
        "libs/orders/src"
      ],
      "@lib/orders/*": [
        "libs/orders/src/*"
      ],
      "@lib/products": [
        "libs/products/src"
      ],
      "@lib/products/*": [
        "libs/products/src/*"
      ],
      "@lib/shared": [
        "libs/shared/src"
      ],
      "@lib/shared/*": [
        "libs/shared/src/*"
      ],
      "@lib/users": [
        "libs/users/src"
      ],
      "@lib/users/*": [
        "libs/users/src/*"
      ],
      "@lib/auth": [
        "libs/auth/src"
      ],
      "@lib/auth/*": [
        "libs/auth/src/*"
      ],
      "@lib/tools": [
        "libs/tools/src"
      ],
      "@lib/tools/*": [
        "libs/tools/src/*"
      ],
      "@lib/recommendations": [
        "libs/recommendations/src"
      ],
      "@lib/recommendations/*": [
        "libs/recommendations/src/*"
      ],
      "@lib/chatbot": [
        "libs/chatbot/src"
      ],
      "@lib/chatbot/*": [
        "libs/chatbot/src/*"
      ],
      "@lib/ai-core": [
        "libs/ai-core/src"
      ],
      "@lib/ai-core/*": [
        "libs/ai-core/src/*"
      ],
      "@lib/context-manager": [
        "libs/context-manager/src"
      ],
      "@lib/context-manager/*": [
        "libs/context-manager/src/*"
      ],
      "@lib/conversation": [
        "libs/conversation/src"
      ],
      "@lib/conversation/*": [
        "libs/conversation/src/*"
      ],
      "@lib/shopping-cart": [
        "libs/shopping-cart/src"
      ],
      "@lib/shopping-cart/*": [
        "libs/shopping-cart/src/*"
      ],
      "@lib/prompts": [
        "libs/prompts/src"
      ],
      "@lib/prompts/*": [
        "libs/prompts/src/*"
      ],
      "@lib/company": [
        "libs/company/src"
      ],
      "@lib/company/*": [
        "libs/company/src/*"
      ],
      "@lib/product-search": [
        "libs/product-search/src"
      ],
      "@lib/product-search/*": [
        "libs/product-search/src/*"
      ]
    }
  }
}